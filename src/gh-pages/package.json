{
    "name": "gh-pages",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack --port 5000",
        "build": "npm run build-rust && next build",
        "build-rust": "cd ../rust && wasm-pack build --target web && rm -rf ../gh-pages/src/models/pkg/ && mv pkg/ ../gh-pages/src/models/",
        "start": "next start",
        "static": "npx serve@latest out",
        "lint": "next lint"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/material": "^7.2.0",
        "next": "15.3.5",
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "eslint": "^9",
        "eslint-config-next": "15.3.5",
        "typescript": "^5"
    }
}
